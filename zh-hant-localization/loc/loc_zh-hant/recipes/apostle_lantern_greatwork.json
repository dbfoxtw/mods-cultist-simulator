                "ingredientlanternf": 1,
                "fragmentmothe": 1,
                "toollanternf": 1,
                "kleidouchos": 1,
                "promise": 1
            },
            "warmup": 60,
            "craftable": true,
            "ending": "majorlanternvictory"
        },
